<launch>

<group ns="uav1">
<node pkg="explore_lite" type="explore" respawn="false" name="explore1" output="screen">
  <param name="robot_base_frame" value="/uav1/base_footprint"/>
  <param name="costmap_topic" value="/uav1/move_base1/global_costmap/costmap"/>
  <param name="costmap_updates_topic" value="map_updates"/>
  <param name="visualize" value="true"/>
  <param name="planner_frequency" value="0.33"/>
  <param name="progress_timeout" value="1000.0"/>
  <param name="potential_scale" value="3.0"/>
  <param name="orientation_scale" value="0.0"/>
  <param name="gain_scale" value="1.0"/>
  <param name="transform_tolerance" value="0.3"/>
  <param name="min_frontier_size" value="0.5"/>
</node>
 </group>


<group ns="uav2">
<node pkg="explore_lite" type="explore" respawn="false" name="explore2" output="screen">
  <param name="robot_base_frame" value="/uav2/base_footprint"/>
  <param name="costmap_topic" value="/uav2/move_base2/global_costmap/costmap"/>
  <param name="costmap_updates_topic" value="map_updates"/>
  <param name="visualize" value="true"/>
  <param name="planner_frequency" value="0.33"/>
  <param name="progress_timeout" value="1000.0"/>
  <param name="potential_scale" value="3.0"/>
  <param name="orientation_scale" value="0.0"/>
  <param name="gain_scale" value="1.0"/>
  <param name="transform_tolerance" value="0.3"/>
  <param name="min_frontier_size" value="0.5"/>
</node>
</group>
</launch>