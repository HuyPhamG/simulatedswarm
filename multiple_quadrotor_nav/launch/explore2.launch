<launch>
<group ns="uav2">
<node pkg="explore_lite" type="explore" respawn="false" name="explore2" output="screen">
  <param name="robot_base_frame" value="/uav2/base_footprint"/>
  <param name="costmap_topic" value="/uav2/move_base2/global_costmap/costmap"/>
  <param name="costmap_updates_topic" value="/uav2/map_updates"/>
  <param name="visualize" value="true"/>
  <param name="planner_frequency" value="2.0"/>
  <param name="progress_timeout" value="1000.0"/>
  <param name="potential_scale" value="0.001"/>
  <param name="orientation_scale" value="0.0"/>
  <param name="gain_scale" value="1.3"/>
  <param name="transform_tolerance" value="0.3"/>
  <param name="min_frontier_size" value="1.0"/>
</node>
</group>
</launch>